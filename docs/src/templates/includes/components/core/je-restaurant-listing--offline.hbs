<section class="c-listing-item" itemscope="" itemtype="http://schema.org/Restaurant" data-restaurant-id="123" data-test-id="123">
    <a href="#" class="c-listing-item-link u-clearfix" itemscope="" itemtype="http://schema.org/Restaurant"
       title="View the Menu for Fozzie Kebab">
        <div class="c-listing-item-img">
            <img class="js-lazy c-listing-item-imgItem" data-ft="restaurantDetailsLogo" src="{{ baseUrl }}/assets/img/examples/restaurant-logo.gif" alt="Fozzie Kebab" />
            <noscript>img class="c-restaurant__logo js-lazy" data-ft="restaurantDetailsLogo" src="{{ baseUrl }}/assets/img/examples/restaurant-logo.gif" alt="Fozzie Kebab" itemprop="image";</noscript>
        </div>
        <div class="c-listing-item-meta">
            <span class="c-listing-item-label c-listing-item-flag">20% off</span>
        </div>
        <div class="c-listing-item-info">
            <h3 class="c-listing-item-title" itemprop="name">Fozzie Kebab</h3>
            <div class="c-listing-item-rating" itemprop="aggregateRating" itemscope="" itemtype="http://schema.org/AggregateRating">
                {{> je-star-ratings size-class="c-listing-item-ratingStars" star-rating='55' }}
                <span class="c-listing-ratingText">(150)</span>
                <meta itemprop="ratingValue" content="5.5">
                <meta itemprop="itemreviewed" content="">
                <meta itemprop="ratingCount" content="150">
                <meta itemprop="bestRating" content="6">
                <meta itemprop="worstRating" content="0">
            </div>
            <p class="c-listing-item-text" itemprop="servesCuisine"><strong>Cuisine Type</strong></p>
            <p class="c-listing-item-text" itemprop="address" itemscope="" itemtype="http://schema.org/PostalAddress">123 Fake Street, Bristol</p>
        </div>
        <div class="c-listing-item-details">
            <p class="c-listing-item-text">Delivery fee: £1</p>
            <p class="c-listing-item-text">Spend £10 for delivery</p>
        </div>
    </a>
</section>
